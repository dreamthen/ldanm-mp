{
	"name": "ldm-taro-frc",
	"version": "0.7.5",
	"description": "拉旦木小程序-功能组件(基于taro)",
	"templateInfo": {
		"name": "default",
		"typescript": false,
		"css": "sass"
	},
	"keywords": [
		"ldm",
		"taro",
		"mp",
		"frc"
	],
	"main": "lib/index.js",
	"files": [
		"lib/**"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dreamthen/ldanm-mp.git"
	},
	"bugs": {
		"url": "https://github.com/dreamthen/ldanm-mp/issues"
	},
	"homepage": "https://github.com/dreamthen/ldanm-mp#readme",
	"scripts": {
		"build:component": "node scripts/build.js",
		"build:weapp": "NODE_ENV=production taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"prepublish": "yarn run build:component"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@tarojs/components": "3.4.4",
		"@tarojs/plugin-framework-react": "3.4.4",
		"@tarojs/react": "3.4.4",
		"@tarojs/router": "3.4.4",
		"@tarojs/runtime": "3.4.4",
		"@tarojs/taro": "3.4.4",
		"@tarojs/taro-h5": "3.4.4",
		"classnames": "^2.3.1",
		"moment": "^2.29.1",
		"prop-types": "^15.8.1",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-redux": "^7.2.7",
		"redux": "^4.1.2",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.4.1",
		"regenerator-runtime": "^0.13.9",
		"taro-ui": "^3.1.0-beta.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.8",
		"@babel/plugin-proposal-class-properties": "^7.16.7",
		"@babel/plugin-proposal-decorators": "^7.17.8",
		"@babel/plugin-proposal-object-rest-spread": "^7.17.3",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@babel/runtime": "^7.17.8",
		"@tarojs/mini-runner": "3.4.4",
		"@tarojs/webpack-runner": "3.4.4",
		"@types/react": "^17.0.43",
		"@types/webpack-env": "^1.16.3",
		"babel-eslint": "^10.1.0",
		"babel-preset-taro": "3.4.4",
		"eslint": "^8.12.0",
		"eslint-config-taro": "3.4.4",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"ora": "^6.1.0",
		"shelljs": "^0.8.5",
		"stylelint": "^14.6.1",
		"webpack-bundle-analyzer": "^4.5.0",
		"yargs": "^17.4.0"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	]
}
